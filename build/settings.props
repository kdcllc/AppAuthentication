<Project>

  <PropertyGroup Label="Basic Settings">
    <VersionPrefix>5.0.0-preview1</VersionPrefix>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
    <LangVersion>preview</LangVersion>
    <Nullable>disable</Nullable>
    <NoWarn>$(NoWarn);CS1591;NU1605;</NoWarn>
  </PropertyGroup>


  <PropertyGroup Label="Ownership Settings">
    <Company>King David Consulting LLC</Company>
    <Authors>kdcllc</Authors>
    <Copyright>Copyright Â© King David Consulting LLC</Copyright>
  </PropertyGroup>

  <PropertyGroup Label="Nuget Package Settings">
    <Description>DoNetCore CLI tool for allowing local Docker Containers access to Azure Resources.</Description>
    <PackageProjectUrl>https://github.com/kdcllc/AppAuthentication</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kdcllc/AppAuthentication.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicense>https://github.com/kdcllc/AppAuthentication/master/LICENSE</PackageLicense>

    <!-- https://docs.microsoft.com/en-us/nuget/reference/errors-and-warnings/nu5048 -->
    <PackageIcon>icon.png</PackageIcon>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Label="SourceLink Settings">
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>

    <DebugType>Embedded</DebugType>
    <EmbedAllSources>True</EmbedAllSources>
  </PropertyGroup>
</Project>
